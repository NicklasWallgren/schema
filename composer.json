{
    "name": "oli/schema",
    "description": "Object oriented GraphQL schema",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "oligus",
            "email": "oli.gustafsson@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/collections": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "spatie/phpunit-watcher": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "GQLSchema\\": "src/"
